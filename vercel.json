{
  "version": 2,
  "builds": [
    {
      "src": "src/index.js",
      "use": "@vercel/node",
      "config": { 
        "includeFiles": ["src/**", "settings.js", "package.json"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/src/index.js"
    }
  ],
  "env": {
    "USE_STDIO": "false",
    "USE_HTTP": "true"
  }
}
